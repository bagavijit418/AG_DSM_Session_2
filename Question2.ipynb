   "source": [
    "Create the below pattern using nested for loop in Python.\n",
    "\n",
    "*\n",
    "* *\n",
    "* * *\n",
    "* * * *\n",
    "* * * * *\n",
    "* * * *\n",
    "* * *\n",
    "* *\n",
    "*"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "* \n",
      "* * \n",
      "* * * \n",
      "* * * * \n",
      "* * * * * \n",
      "* * * * \n",
      "* * * \n",
      "* * \n",
      "* \n"
     ]
    }
   ],
   "source": [
    "level= 5\n",
    "chrctr= \"* \"\n",
    "for i in range(1, level+1):\n",
    "    out=\"\"\n",
    "    for j in range(i):\n",
    "           out+=chrctr\n",
    "    print(out)\n",
    "for i in range(level-1,0,-1):\n",
    "    out=\"\"\n",
    "    for j in range(i):\n",
    "           out+=chrctr\n",
    "    print(out)"
   ]
  },
